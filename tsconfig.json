{
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"target": "es2022",
		"allowJs": true,
		"resolveJsonModule": true,
		"moduleDetection": "force",
		"isolatedModules": true,
		"verbatimModuleSyntax": true,
		"declaration": true,
		"module": "NodeNext",
		"outDir": "dist",
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"noImplicitOverride": true
	}
}
